local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/imhenne187/SilenceElerium/refs/heads/main/src/SilenceEleriumLibrary.luau"))()

local window = library:AddWindow("INV Hub | Premium", {
	title_bar = {Color3.fromRGB(128, 128, 128), Color3.fromRGB(0, 0, 0), Color3.fromRGB(50, 0, 0)},
	title_bar_transparency = 0.2,
	background = {Color3.fromRGB(0, 0, 0), Color3.fromRGB(34, 0, 0), Color3.fromRGB(68, 0, 0)},
	background_transparency = 0.1,
	main_color = Color3.fromRGB(31, 81, 255),
	min_size = Vector2.new(400, 300),
	can_resize = true
})

local main = window:AddTab("Main")
main:Show()

local performance = window:AddTab("Performance")
performance:Show()

local killer = window:AddTab("Killer")
killer:Show()

local rebirths = window:AddTab("Rebirths")
rebirths:Show()

local autofarm = window:AddTab("Auto Farm")
autofarm:Show()

local fastfarming = window:AddTab("Fast Farming")
fastfarming:Show()

local extras = window:AddTab("Extras")
extras:Show()

local discord = window:AddTab("Discord")
discord:Show()

local Godlikeshop = window:AddTab("Godlikes Shop")
Godlikeshop:Show()

local fastrebirthSwitch = rebirths:AddSwitch("Fast Rebirth Adaption", function(state)
	if state then
		spawn(function()
			local ReplicatedStorage = game:GetService("ReplicatedStorage")
			local Players = game:GetService("Players")
			local VirtualInputManager = game:GetService("VirtualInputManager")
			local LocalPlayer = Players.LocalPlayer

			local unequipAll = function()
				local petsFolder = LocalPlayer.petsFolder
				for _, folder in pairs(petsFolder:GetChildren()) do
					if folder:IsA("Folder") then
						for _, pet in pairs(folder:GetChildren()) do
							ReplicatedStorage.rEvents.equipPetEvent:FireServer("unequipPet", pet)
						end
					end
				end
				task.wait(0.1)
			end

			local equipMultiple = function(petsTable)
				for _, petName in pairs(petsTable) do
					for _, pet in pairs(LocalPlayer.petsFolder.Unique:GetChildren()) do
						if pet.Name == petName then
							ReplicatedStorage.rEvents.equipPetEvent:FireServer("equipPet", pet)
						end
					end
				end
			end

			local equipSingle = function(petName)
				for _, pet in pairs(LocalPlayer.petsFolder.Unique:GetChildren()) do
					if pet.Name == petName then
						ReplicatedStorage.rEvents.equipPetEvent:FireServer("equipPet", pet)
						break
					end
				end
			end

			local findMachine = function(name)
				local machine = workspace.machinesFolder:FindFirstChild(name)
				if not machine then
					for _, folder in pairs(workspace:GetChildren()) do
						if folder:IsA("Folder") and folder.Name:find("machines") then
							machine = folder:FindFirstChild(name)
							if machine then break end
						end
					end
				end
				return machine
			end

			local pressE = function()
				VirtualInputManager:SendKeyEvent(true, "E", false, game)
				task.wait(0.1)
				VirtualInputManager:SendKeyEvent(false, "E", false, game)
			end

			while task.wait() and fastrebirthSwitch:Get() do
				local currentRebirths = LocalPlayer.leaderstats.Rebirths.Value
				local targetStrength = 10000 + (5000 * currentRebirths)

				if LocalPlayer.ultimatesFolder:FindFirstChild("Golden Rebirth") then
					local multiplier = LocalPlayer.ultimatesFolder["Golden Rebirth"].Value
					targetStrength = math.floor(targetStrength * (1 - (multiplier * 0.1)))
				end

				unequipAll()

				equipMultiple({"Swift Samurai", "Darkstar Hunter"})

				while LocalPlayer.leaderstats.Strength.Value < targetStrength and fastrebirthSwitch:Get() do
					for i = 1, 12 do
						LocalPlayer.muscleEvent:FireServer("rep")
					end
					task.wait()
				end

				unequipAll()

				equipSingle("Tribal Overlord")

				local machine = findMachine("Jungle Bar Lift")
				if machine and machine:FindFirstChild("interactSeat") then
					LocalPlayer.Character.HumanoidRootPart.CFrame = machine.interactSeat.CFrame * CFrame.new(0, 3, 0)
					repeat
						task.wait(0.1)
						pressE()
					until LocalPlayer.Character.Humanoid.Sitting
				end

				local oldRebirths = LocalPlayer.leaderstats.Rebirths.Value
				repeat
					ReplicatedStorage.rEvents.rebirthRemote:InvokeServer("rebirthRequest")
					task.wait(0.1)
				until LocalPlayer.leaderstats.Rebirths.Value > oldRebirths

				task.wait()
			end
		end)
	else
		-- Loop stops automatically when switch is off due to Get() check
	end
end)
